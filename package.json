    "less-loader": "^11.1.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "^0.24.0",
    "pinia": "^2.0.28",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
